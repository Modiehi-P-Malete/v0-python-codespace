   ],
   "source": [
    "def average(a, b, c):\n",
    "    return (a + b + c) // 3\n",
    "\n",
    "print(average(3, 10, 8))    # 7\n",
    "print(average(10, 5, 12))   # 9\n",
    "print(average(6, 20, 40))   # 22"
   ]
  }
